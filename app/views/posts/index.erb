<div class="row">
  <div class="col s12 m9 l8 offset-l1">
    <div class="row newsplatepostit grey lighten-5">
      <i class="pin"></i>
      <p style="font-size: 20px; margin-left:1em;"><b>Agenda :&nbsp;&nbsp;<a class="waves-effect waves-light btn" style="width:30%; background-color:#81d4fa;"><%= @agenda.name %></a></b></p>
      <!-- Agenda를 수정하는 부분 -->
      <div class="fixed-action-btn horizontal" style="top:1em; right:1em;">
        <a class="btn-floating">
          <i class="large material-icons yellow darken-1">mode_edit</i>
        </a>
        <ul>
          <li><a class="btn-floating yellow darken-1" href="<%= edit_agenda_path(@agenda.id) %>"><i class="material-icons">mode_edit</i></a></li>
          <li><a class="btn-floating red" href="<%= agenda_path(@agenda.id) %>" data-method="delete"><i class="material-icons">delete</i></a></li>
        </ul>
      </div>

      <!--postcard type2가 들어있는 div-->
      <div class="col s12">
        <div class="row">
            <% if user_signed_in? %>
              <% @posts.each do |p| %>
               <%= render partial: "postcard2", locals: {p: p} %>  
              <% end %>
            <% end %>
        </div>
      </div>
      
    </div>
  </div>
  <div class="col hide-on-small-only m3 l2">
    <%= render partial: "posts/post_curation" %>
  </div>
</div>

