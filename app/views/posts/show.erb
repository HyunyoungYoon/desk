<!--post의 내용-->
<%= @post.url %>
<%= @post.title %>
<%= @post.agenda.name %>
<%#= @post.pic %>
<%= @post.abstract %>

<!--자신이 쓴 코멘트-->
<%= @post.my_comment.content %></br>
<!--share한 다른 user의 코멘트-->
<% @post.shared_comments.all.each do |comment| %>
    <%= comment.writer %></br>
    <%= comment.content %></br>
<% end %>
<!--같은 링크에 대한 다른 user의 코멘트-->
<%= @post.same %>
<% @post.same.all.each do |same| %>
    <%#= same.user_id %></br>
    <%= same.my_comment.content if same.my_comment%></br>
<% end %>